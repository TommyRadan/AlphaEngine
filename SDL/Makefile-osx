# Makefile to build the SDL library
# This file MUST be executed with working directory in SDL root

BIN_DIR = bin

CC      = gcc

CFLAGS  = -g -O3 -DUSING_GENERATED_CONFIG_H
EXTRA_CFLAGS = -Iinclude -mmmx -m3dnow -msse -msse2 -DTARGET_API_MAC_CARBON -DTARGET_API_MAC_OSX -fvisibility=hidden -D_THREAD_SAFE -fpascal-strings -Wno-deprecated-declarations

LDFLAGS = 
EXTRA_LDFLAGS =  -lm -liconv  -Wl,-framework,ForceFeedback -lobjc -Wl,-framework,CoreVideo -Wl,-framework,Cocoa -Wl,-framework,Carbon -Wl,-framework,IOKit -Wl,-framework,CoreAudio -Wl,-framework,AudioToolbox -Wl,-framework,AudioUnit

TARGET  = SDL2.so
OBJECTS = $(BIN_DIR)/SDL.o $(BIN_DIR)/SDL_assert.o $(BIN_DIR)/SDL_error.o $(BIN_DIR)/SDL_hints.o $(BIN_DIR)/SDL_log.o $(BIN_DIR)/SDL_atomic.o $(BIN_DIR)/SDL_spinlock.o $(BIN_DIR)/SDL_audio.o $(BIN_DIR)/SDL_audiocvt.o $(BIN_DIR)/SDL_audiodev.o $(BIN_DIR)/SDL_audiotypecvt.o $(BIN_DIR)/SDL_mixer.o $(BIN_DIR)/SDL_wave.o $(BIN_DIR)/SDL_cpuinfo.o $(BIN_DIR)/SDL_dynapi.o $(BIN_DIR)/SDL_clipboardevents.o $(BIN_DIR)/SDL_dropevents.o $(BIN_DIR)/SDL_events.o $(BIN_DIR)/SDL_gesture.o $(BIN_DIR)/SDL_keyboard.o $(BIN_DIR)/SDL_mouse.o $(BIN_DIR)/SDL_quit.o $(BIN_DIR)/SDL_touch.o $(BIN_DIR)/SDL_windowevents.o $(BIN_DIR)/SDL_rwops.o $(BIN_DIR)/SDL_haptic.o $(BIN_DIR)/SDL_gamecontroller.o $(BIN_DIR)/SDL_joystick.o $(BIN_DIR)/e_atan2.o $(BIN_DIR)/e_log.o $(BIN_DIR)/e_pow.o $(BIN_DIR)/e_rem_pio2.o $(BIN_DIR)/e_sqrt.o $(BIN_DIR)/k_cos.o $(BIN_DIR)/k_rem_pio2.o $(BIN_DIR)/k_sin.o $(BIN_DIR)/k_tan.o $(BIN_DIR)/s_atan.o $(BIN_DIR)/s_copysign.o $(BIN_DIR)/s_cos.o $(BIN_DIR)/s_fabs.o $(BIN_DIR)/s_floor.o $(BIN_DIR)/s_scalbn.o $(BIN_DIR)/s_sin.o $(BIN_DIR)/s_tan.o $(BIN_DIR)/SDL_power.o $(BIN_DIR)/SDL_d3dmath.o $(BIN_DIR)/SDL_render.o $(BIN_DIR)/SDL_yuv_mmx.o $(BIN_DIR)/SDL_yuv_sw.o $(BIN_DIR)/SDL_render_d3d.o $(BIN_DIR)/SDL_render_d3d11.o $(BIN_DIR)/SDL_render_gl.o $(BIN_DIR)/SDL_shaders_gl.o $(BIN_DIR)/SDL_render_gles.o $(BIN_DIR)/SDL_render_gles2.o $(BIN_DIR)/SDL_shaders_gles2.o $(BIN_DIR)/SDL_render_psp.o $(BIN_DIR)/SDL_blendfillrect.o $(BIN_DIR)/SDL_blendline.o $(BIN_DIR)/SDL_blendpoint.o $(BIN_DIR)/SDL_drawline.o $(BIN_DIR)/SDL_drawpoint.o $(BIN_DIR)/SDL_render_sw.o $(BIN_DIR)/SDL_rotate.o $(BIN_DIR)/SDL_getenv.o $(BIN_DIR)/SDL_iconv.o $(BIN_DIR)/SDL_malloc.o $(BIN_DIR)/SDL_qsort.o $(BIN_DIR)/SDL_stdlib.o $(BIN_DIR)/SDL_string.o $(BIN_DIR)/SDL_thread.o $(BIN_DIR)/SDL_timer.o $(BIN_DIR)/SDL_RLEaccel.o $(BIN_DIR)/SDL_blit.o $(BIN_DIR)/SDL_blit_0.o $(BIN_DIR)/SDL_blit_1.o $(BIN_DIR)/SDL_blit_A.o $(BIN_DIR)/SDL_blit_N.o $(BIN_DIR)/SDL_blit_auto.o $(BIN_DIR)/SDL_blit_copy.o $(BIN_DIR)/SDL_blit_slow.o $(BIN_DIR)/SDL_bmp.o $(BIN_DIR)/SDL_clipboard.o $(BIN_DIR)/SDL_egl.o $(BIN_DIR)/SDL_fillrect.o $(BIN_DIR)/SDL_pixels.o $(BIN_DIR)/SDL_rect.o $(BIN_DIR)/SDL_shape.o $(BIN_DIR)/SDL_stretch.o $(BIN_DIR)/SDL_surface.o $(BIN_DIR)/SDL_video.o $(BIN_DIR)/SDL_nullevents.o $(BIN_DIR)/SDL_nullframebuffer.o $(BIN_DIR)/SDL_nullvideo.o $(BIN_DIR)/SDL_diskaudio.o $(BIN_DIR)/SDL_dummyaudio.o $(BIN_DIR)/SDL_sysloadso.o $(BIN_DIR)/SDL_cocoaclipboard.o $(BIN_DIR)/SDL_cocoaevents.o $(BIN_DIR)/SDL_cocoakeyboard.o $(BIN_DIR)/SDL_cocoamessagebox.o $(BIN_DIR)/SDL_cocoamodes.o $(BIN_DIR)/SDL_cocoamouse.o $(BIN_DIR)/SDL_cocoamousetap.o $(BIN_DIR)/SDL_cocoaopengl.o $(BIN_DIR)/SDL_cocoashape.o $(BIN_DIR)/SDL_cocoavideo.o $(BIN_DIR)/SDL_cocoawindow.o $(BIN_DIR)/SDL_systhread.o $(BIN_DIR)/SDL_syssem.o $(BIN_DIR)/SDL_sysmutex.o $(BIN_DIR)/SDL_syscond.o $(BIN_DIR)/SDL_systls.o $(BIN_DIR)/SDL_coreaudio.o $(BIN_DIR)/SDL_sysjoystick.o $(BIN_DIR)/SDL_syshaptic.o $(BIN_DIR)/SDL_syspower.o $(BIN_DIR)/SDL_sysfilesystem.o $(BIN_DIR)/SDL_systimer.o $(BIN_DIR)/SDL_rwopsbundlesupport.o

all: make_build_dir ../$(BIN_DIR)/$(TARGET)

rebuild: clean all

make_build_dir:
	@mkdir -p $(BIN_DIR)
	@mkdir -p ../$(BIN_DIR)/$(TARGET)

../$(BIN_DIR)/$(TARGET): $(OBJECTS)
	@echo "  Linking SDL  "; $(CC) -shared -o $@ $(OBJECTS) $(LDFLAGS) $(EXTRA_LDFLAGS)
	@echo "  SDL Compilation and Linking done  "

clean:
	@rm -rf $(BIN_DIR)
	@echo "  SDL binaries removed  "

$(BIN_DIR)/SDL.o: SDL.c
	@echo "  Compiling $?  "; $(CC) -c $(CFLAGS) $(EXTRA_CFLAGS) -o $@ $?

